{"ast":null,"code":"import '../styles/TheHeader.css';","map":{"version":3,"names":[],"sources":["D:\\BSTU\\2course\\2term\\ТРПИ\\vue\\src\\components\\TheHeader.vue"],"sourcesContent":["<template>\r\n  <header class=\"header\">\r\n    <div class=\"background\"></div>\r\n    <div class=\"wrapper\">\r\n      <section class=\"contacts\">\r\n        <div>\r\n          <!-- как им сделать разные ссылки?? -->\r\n          <a class=\"language\" v-for=\"language in languages\" :key=\"language\">\r\n            {{ language.name }}\r\n          </a>\r\n        </div>\r\n        <a href=\"tel:8 (800) 123-45-67\" class=\"phoneNumber\">\r\n          {{ phone }}\r\n        </a>\r\n        <a href=\"mailto:sales@log.ru\" class=\"email\">\r\n          {{ email }}\r\n        </a>\r\n      </section>\r\n      <section>\r\n        <div class=\"menu-items\">\r\n          <div class=\"flexstarItems\">\r\n            <img src=\"../images/logo.svg\" class=\"logo\" />\r\n            <img\r\n              src=\"../images/placeholder.svg\"\r\n              class=\"placemark\"\r\n              :class=\"{ 'placemark-hover': isHovered }\"\r\n            />\r\n            <a\r\n              href=\"#\"\r\n              @mouseover=\"isHovered = true\"\r\n              @mouseleave=\"isHovered = false\"\r\n              >Наши офисы</a\r\n            >\r\n          </div>\r\n          <div class=\"flexendItems\">\r\n            <a\r\n              href=\"{{item.id}}\"\r\n              class=\"navigation\"\r\n              v-for=\"item in navigation\"\r\n              :key=\"item\"\r\n            >\r\n              {{ item.name }}\r\n            </a>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nimport '../styles/TheHeader.css'\r\n</script>\r\n"],"mappings":"AAmDA,OAAO,yBAAwB"},"metadata":{},"sourceType":"module","externalDependencies":[]}