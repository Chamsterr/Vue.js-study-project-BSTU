    icon: L.icon({\r\n          iconUrl: './src/images/marker_map_icon.png',\r\n          iconSize: [38, 95],\r\n          iconAnchor: [22, 94],\r\n          popupAnchor: [-3, -76]\r\n        })\r\n      }).addTo(this.map);\r\n\r\n      this.map.on('layeradd', function(event) {\r\n        console.log('Layer added', event);\r\n      });\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style src=\"../styles/TheFooter.css\"></style>"],"mappings":";;EAESA,KAAK,EAAC,eAAe;EAACC,GAAG,EAAC;;;uBADjCC,mBAAA,CAEQ,cADNC,mBAAA,CAA2C,OAA3CC,UAA2C,8B"},"metadata":{},"sourceType":"module","externalDependencies":[]}